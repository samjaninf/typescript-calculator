{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "interface-name": [
      false
    ],
    "interface-over-type-literal": false,
    "ordered-imports": [
      true,
      {
        "import-sources-order": [
          false
        ],
        "named-imports-order": [
          true,
          "lowercase-last"
        ]
      }
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-escape"
    ],
    "semicolon": [
      true,
      "never"
    ]
  }
}
